{
    "project_title": "Text Counter",
    "project_description": "You can choose whether space, enter or symbols should be count or not.",
    "author_info": "author: Ellie Yen, 2021",
    "limit": 100,
    "cur_length": 0,
    "content": {
        "all": "", 
        "safe": [], 
        "overflow": []
